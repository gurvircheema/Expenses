<h1>Log in</h1>
<%= form_tag sessions_path do %>
	<p>
		<%= label_tag :email %><br/>
		<%= text_field_tag :email %>
	</p>
	<p>
		<%= label_tag :password %><br/>
		<%= password_field_tag :password %>
	</p>
	<p>
		<%= submit_tag "Log in" %>
	</p>
<% end %>
<p><%= link_to "Back" ,root_url, class: "bg-info" %></p>